import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner ab=new Scanner(System.in);
        int x1=ab.nextInt();
        int y1=ab.nextInt();
        int x2=ab.nextInt();
        int y2=ab.nextInt();
        double a=(x1+x2)/2.0;
        double b=(y1+y2)/2.0;
        System.out.printf("Binoy's house is located at (%.1f,%.1f)",a,b);
        
    }
}
