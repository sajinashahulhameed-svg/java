import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner DF=new Scanner(System.in);
        double f=DF.nextDouble();
        int n1=DF.nextInt();//Integer for sqrt
        int n2=DF.nextInt();//Base for power
        int n3=DF.nextInt();//Exponent for power
        System.out.println((int)Math.floor(f));//Floor value
        System.out.println((int)Math.ceil(f));//Ceil value
        System.out.println((int)Math.sqrt(n1));//Square root
        System.out.println((int)Math.pow(n2,n3));//Power value
        DF.close();
    }
}
